{
  "name": "craftcms/store-hours",
  "description": "This plugin adds a new “Store Hours” field type to Craft, for collecting the opening and closing hours of a business for each day of the week.",
  "version": "2.0.0",
  "type": "craft-plugin",
  "keywords": [
    "html",
    "hours",
    "store",
    "cms",
    "craftcms",
    "json",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel & Tonic",
      "homepage": "https://pixelandtonic.com/"
    }
  ],
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/store-hours/issues?state=open",
    "source": "https://github.com/craftcms/store-hours",
    "docs": "https://github.com/craftcms/store-hours",
    "rss": "https://github.com/craftcms/store-hours/commits/master.atom"
  },
  "require": {
    "craftcms/cms": "~3.0.0-beta.1"
  },
  "autoload": {
    "psr-4": {
      "craft\\storehours\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "name": "Store Hours",
    "handle": "storeHours",
    "changelogUrl": "https://raw.githubusercontent.com/craftcms/store-hours/master/CHANGELOG.md",
    "downloadUrl": "https://github.com/craftcms/store-hours/archive/master.zip"
  }
}
